<?xml version="1.0" encoding="ISO-8859-1"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2" name="Cumprimento de sentença"> 
    <!-- SWIMLANES -->
    <swimlane name="Nó de Desvio - Cumprimento de sentença">
        <assignment pooled-actors="#{localizacaoAssignment.getPooledActors('6:1338,5:1469,35627:1338,35624:1338,35626:1469,35623:1469')}"/>
    </swimlane>
    <swimlane name="Assessoria">
        <assignment pooled-actors="#{localizacaoAssignment.getPooledActors('4:5197,5:1469,6:1338,8068:5197,8067:1469,8066:1338,35625:5197,35622:5197,35623:1469,35626:1469,35624:1338,35627:1338')}"/>
    </swimlane>
    <swimlane name="Secretaria">
        <assignment pooled-actors="#{localizacaoAssignment.getPooledActors('6:1338,6:5483,8066:1338,8066:5483,35627:1338,35627:5483,35624:1338,35624:5483')}"/>
    </swimlane>
    <swimlane name="Gabinete">
        <assignment pooled-actors="#{localizacaoAssignment.getPooledActors('5:1469,6:1338,8067:1469,8066:1338,35627:1338,35624:1338,35626:1469,35623:1469')}"/>
    </swimlane>  
    <!-- START-STATE -->
    <start-state name="Início">
        <task name="Tarefa inicial" swimlane="Secretaria" priority="3"/>
        <transition to="Despachar processo CS" name="Despachar processo CS"/>
    </start-state>  
    <!-- NODES -->
    <process-state name="Despachar processo CS">
        <sub-process name="Preparar ato judicial" binding="late"/>
        <transition to="Verificar providência a adotar CS" name="Verificar providência a adotar CS"/>
    </process-state>
    <task-node end-tasks="true" name="Verificar providência a adotar CS">
        <task name="Verificar providência a adotar CS" swimlane="Secretaria" priority="3">
            <controller>
                <variable name="MovimentarLote" mapped-name="movimentarLote:MovimentarLote" access="read,write"/>
                <variable name="Processo_Fluxo_visualizarDecisao" mapped-name="frame:Processo_Fluxo_visualizarDecisao" access="read,write"/>
            </controller>
        </task>
        <transition to="Nó de Desvio - Cumprimento de sentença" name="Nó de Desvio - Cumprimento de sentença">
            <condition expression="#{true}"/>
        </transition>
        <transition to="Dar cumprimento a ato judicial CS" name="Dar cumprimento a ato judicial CS"/>
        <transition to="Julgar CS" name="Julgar CS"/>
        <transition to="Término" name="Encerrar fluxo de execução"/>
        <transition to="Despachar processo CS" name="Despachar processo CS"/>
        <transition to="Arquivar CS" name="Arquivar CS"/>
        <transition to="Verificar providências finais CS" name="Verificar providências finais CS"/>
        <transition to="Remeter ao 2o grau" name="Remeter ao 2o grau"/>
        <transition to="Expedir Alvará CS" name="Expedir Alvará CS"/>
        <transition to="Controle de audiência" name="Controle de audiência"/>
        <transition to="Inserir no novo fluxo" name="Inserir no novo fluxo"/>
    </task-node>
    <process-state name="Inserir no novo fluxo">
        <sub-process name="Fluxo básico de conhecimento JEC" binding="late"/>
        <transition to="Verificar providência a adotar CS" name="Verificar providência a adotar CS"/>
    </process-state>
    <process-state name="Dar cumprimento a ato judicial CS">
        <sub-process name="Cumprimento de decisão" binding="late"/>
        <transition to="Verificar providência a adotar CS" name="Verificar providência a adotar CS"/>
    </process-state>
    <process-state name="Julgar CS">
        <sub-process name="Preparar ato judicial" binding="late"/>
        <transition to="Verificar providência a adotar CS" name="Verificar providência a adotar CS"/>
    </process-state>
    <process-state name="Arquivar CS">
        <sub-process name="Arquivamento" binding="late"/>
        <transition to="Término" name="Término"/>
    </process-state>
    <task-node end-tasks="true" name="Designar audiência CS">
        <task name="Designar audiência CS" swimlane="Secretaria" priority="3">
            <controller>
                <variable name="Processo_Fluxo_abaDesignarAudiencia" mapped-name="frame:Processo_Fluxo_abaDesignarAudiencia" access="read,write"/>
            </controller>
        </task>
        <transition to="Nó de Desvio - Cumprimento de sentença" name="Nó de Desvio - Cumprimento de sentença">
            <condition expression="#{true}"/>
        </transition>
        <transition to="Controle de audiência" name="Controle de audiência"/>
    </task-node>
    <process-state name="Citar para audiência CS">
        <sub-process name="Preparar ato de comunicação" binding="late"/>
        <transition to="Controle de audiência" name="Controle de audiência"/>
    </process-state>
    <task-node end-tasks="true" name="Aguardar audiência CS">
        <task name="Aguardar audiência CS" swimlane="Secretaria" priority="3"/>
        <transition to="Nó de Desvio - Cumprimento de sentença" name="Nó de Desvio - Cumprimento de sentença">
            <condition expression="#{true}"/>
        </transition>
        <transition to="Controle de audiência" name="Controle de audiência"/>
    </task-node>
    <task-node end-tasks="true" name="Realizar audiência CS">
        <task name="Realizar audiência CS" swimlane="Secretaria" priority="3">
            <controller>
                <variable name="Processo_Fluxo_abaDesignarAudiencia" mapped-name="frame:Processo_Fluxo_abaDesignarAudiencia" access="read,write"/>
            </controller>
        </task>
        <transition to="Nó de Desvio - Cumprimento de sentença" name="Nó de Desvio - Cumprimento de sentença">
            <condition expression="#{true}"/>
        </transition>
        <transition to="Controle de audiência" name="Controle de audiência"/>
    </task-node>
    <process-state name="Controle de audiência">
        <sub-process name="Controle de audiência" binding="late"/>
        <transition to="Verificar providência a adotar CS" name="Verificar providência a adotar CS"/>
    </process-state>
    <end-state name="Término"/>
    <task-node end-tasks="true" name="Nó de Desvio - Cumprimento de sentença">
        <task name="Nó de Desvio - Cumprimento de sentença" swimlane="Nó de Desvio - Cumprimento de sentença" priority="3"/>
        <transition to="Término" name="Término"/>
        <transition to="Verificar providência a adotar CS" name="Verificar providência a adotar CS"/>
        <transition to="Designar audiência CS" name="Designar audiência CS"/>
        <transition to="Aguardar audiência CS" name="Aguardar audiência CS"/>
        <transition to="Realizar audiência CS" name="Realizar audiência CS"/>
        <transition to="Confirmar cumprimento de providências finais CS" name="Verificar cumprimento de providências finais CS"/>
    </task-node>
    <process-state name="Verificar providências finais CS">
        <sub-process name="Preparar providências finais" binding="late"/>
        <transition to="Confirmar cumprimento de providências finais CS" name="Verificar cumprimento de providências finais CS"/>
    </process-state>
    <task-node end-tasks="true" name="Confirmar cumprimento de providências finais CS">
        <task name="Verificar cumprimento de providências finais CS" swimlane="Gabinete" priority="3"/>
        <transition to="Verificar providência a adotar CS" name="Verificar providência a adotar CS"/>
        <transition to="Arquivar CS" name="Arquivar CS"/>
        <transition to="Nó de Desvio - Cumprimento de sentença" name="Nó de Desvio - Cumprimento de sentença">
            <condition expression="#{true}"/>
        </transition>
    </task-node>
    <process-state name="Remeter ao 2o grau">
        <sub-process name="Remessa ao 2 grau (Migração)" binding="late"/>
        <transition to="Verificar providência a adotar CS" name="Verificar providência a adotar CS"/>
    </process-state>
    <process-state name="Expedir Alvará CS">
        <sub-process name="Preperar alvará judicial" binding="late"/>
        <transition to="Verificar providência a adotar CS" name="Verificar providência a adotar CS"/>
    </process-state>  
    <!-- PROCESS-EVENTS -->
    <event type="superstate-enter">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="process-start">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="before-signal">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-end">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="subprocess-created">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-create">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="transition">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-assign">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="after-signal">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="timer">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-start">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="subprocess-end">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="node-leave">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="process-end">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="superstate-leave">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="node-enter">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event> 
</process-definition>
